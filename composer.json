{
    "name": "jadjoubran/laravel5-angular-material-starter",
    "description": "Laravel 5 angular material starter project with elixir setup",
    "keywords": ["laravel", "angular", "material", "starter", "ui-router", "api"],
    "license": "MIT",
    "type": "project",
    "repositories": [
        {
            "type": "vcs",
            "url": "http://github.com/magnolo/laravel-postgis"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "laravel/framework": "5.2.*",
        "barryvdh/laravel-debugbar": "~2.0",
        "tymon/jwt-auth": "0.5.*",
        "dingo/api": "1.0.x@dev",
        "barryvdh/laravel-cors": "0.7.x",
        "doctrine/dbal": "~2.5.2",
        "zizaco/entrust": "dev-laravel-5",
        "flynsarmy/csv-seeder": "1.*",
        "guzzlehttp/guzzle": "^6.1",
        "guzzlehttp/oauth-subscriber": "^0.3.0",
        "firebase/php-jwt": "^3.0",
        "orchestra/imagine": "~3.1",
        "phaza/laravel-postgis": "dev-dev-23"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~4.0",
        "phpspec/phpspec": "~2.1",
        "symfony/dom-crawler": "~3.0",
        "symfony/css-selector": "~3.0"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "scripts": {
        "post-install-cmd": [
            "php artisan clear-compiled",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "php artisan clear-compiled",
            "php artisan optimize"
        ],
        "post-create-project-cmd": [
            "php -r \"copy('.env.example', '.env');\"",
            "php -r \"unlink('code_of_conduct.md');\"",
            "php -r \"unlink('CHANGELOG.md');\"",
            "php -r \"unlink('.codeclimate.yml');\"",
            "php -r \"unlink('.travis.yml');\"",
            "php artisan key:generate",
            "php artisan jwt:generate"
        ]
    }
}
